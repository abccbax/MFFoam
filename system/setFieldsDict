FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box (0 0 -0.2) (0.5 2.5 0.2);
        fieldValues
        (
            volScalarFieldValue alpha.air 0.43
            volScalarFieldValue alpha.particles 0.57
        );
    }
);


