FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    min        1e4;
}

limitTparticles
{
    type            limitTemperature;
    selectionMode   all;
    min             270;
    max             4000;
    phase           particles;
}

limitTair
{
    type            limitTemperature;
    selectionMode   all;
    min             270;
    max             4000;
    phase           air;
}

