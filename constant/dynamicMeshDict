FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

topoChanger
{
    type    movingBullet;

    libs    ("libtopoChangers.so");

    //- Motion velocity amplitude
    motionVelAmplitude		(10 0 0);

    //- Motion velocity period
    motionVelPeriod		1;

    //- Current left obstacle position
    leftObstacleEdge		0.5;

    //- Current right obstacle position
    rightObstacleEdge		4;
    
    //- Current upper obstacle position
    upperObstacleEdge		2.5;
    
    //- Minimum grid width
    minMeshWidth		0.03;
    
    left
    {
        minThickness        0.03;
        maxThickness        0.06;
    }

    right
    {
        minThickness        0.03;
        maxThickness        0.13;
    }
}


